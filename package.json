{
  "name": "glutton",
  "version": "1.0.1-beta",
  "private": true,
  "description": "",
  "main": "./build/game.js",
  "author": "Nazariglez <nazari.nz@gmail.com> (http://www.nazariglez.com)",
  "scripts": {
    "start": "pm2 start build/server/app.js -i 8",
    "client": "webpack --config webpack.config.js --progress --inline --colors --watch",
    "mobile": "MOBILE=true webpack --config webpack.config.js --progress --inline --colors --watch",
    "build": "./bin/build.sh",
    "game": "GORACE=halt_on_error=1 go run -race src/goserver/main.go -path=src/goserver -log=trace",
    "core": "GORACE=halt_on_error=1 go run -race src/goserver/main.go -core -port=8000 -path=src/goserver -log=trace",
    "server": "webpack --config webpack.server.config.js --progress --inline --colors --watch",
    "count-commits": "git log --oneline --all | wc -l",
    "count-loc": "sloc --format cli-table ./src"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "brfs": "^1.4.3",
    "json-loader": "^0.5.4",
    "lodash": "^4.15.0",
    "moment": "^2.17.1",
    "moment-duration-format": "^1.3.0",
    "pixi-animationloop": "^1.0.9",
    "pixi-audio": "^0.1.0",
    "pixi-keyboard": "^0.9.4",
    "pixi-particles": "^2.1.0",
    "pixi-tween": "^0.1.2",
    "pixi.js": "^3.0.10",
    "string-replace-webpack-plugin": "0.0.4",
    "tls": "0.0.1",
    "transform-loader": "^0.2.3",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.9",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "bufferutil": "^1.2.1",
    "compression": "^1.6.2",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "obj-pool": "^1.0.0",
    "rbush": "^2.0.1",
    "source-map-support": "^0.4.0",
    "sqlite3": "^3.1.4",
    "usage": "^0.7.1",
    "utf-8-validate": "^1.2.1",
    "uuid": "^2.0.2",
    "uws": "^0.7.9"
  }
}
